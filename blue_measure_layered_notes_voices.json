{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "data": {
    "url": "https://polyrhythm-project.github.io/vegalitetest/129.tsv"
  },
  "transform": [
    {
      "joinaggregate": [{
        "op": "min",
        "field": "position",
        "as": "measureminpos"
      }],
      "groupby": [
        "measure"
      ]
    }
  ],
  "layer": [
    {
      "width": 3000,
      "height": 300,
      "mark": {
        "type": "bar",
        "opacity": 0.9
      },
      "encoding": {
        "x": {
          "field": "position",
          "type": "temporal",
          "axis": {
            "format": "%-Y"
          }
        },
        "y": {
          "field": "notesA",
          "type": "quantitative"
        }
      }
    },
    {
      "width": 3000,
      "height": 300,
      "mark": {
        "type": "bar",
        "opacity": 0.5
      },
      "encoding": {
        "x": {
          "field": "position",
          "type": "temporal",
          "axis": {
            "format": "%-Y"
          }
        },
        "y": {
          "field": "notesB",
          "type": "quantitative"
        },
        "color": {
          "value": "#7EF9FF"
        }
      }
    },
    {
      "width": 3000,
      "height": 300,
      "mark": {
        "type": "rule",
        "opacity": 0.1
      },
      "encoding": {
        "x": {
          "field": "measureminpos",
          "type": "temporal",
          "axis": {
            "format": "%-Y"
          }
        }
      }
    }
  ],
  "config": {
    "range": {
      "category": {
        "scheme": "category10"
      }
    },
    "bar": {
      "continuousBandSize": 16
    }
  }
}
